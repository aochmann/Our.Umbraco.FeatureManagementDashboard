{
  "$schema": "./umbraco/config/appsettings-schema.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    }
  },
  "ConnectionStrings": {
    "umbracoDbDSN": ""
  },
  "Umbraco": {
    "CMS": {
      "Hosting": {
        "Debug": false
      },
      "Content": {
        "ContentVersionCleanupPolicy": {
          "EnableCleanup": true
        }
      },
      "ModelsBuilder": {
        "ModelsMode": "Nothing"
      },
      "Global": {
        "Id": "af261308-8c62-4525-8e66-654a78200a3d"
      }
    }
  },
  "FeaturesManagementDashboard": {
    "Enabled": true
  },
  "FeatureManagement": {
    "HomePage": true,
    "Weather": true,
    "WeatherPage": false
  }
}